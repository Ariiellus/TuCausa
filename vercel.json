{
  "buildCommand": "cd front && npm run build",
  "outputDirectory": "front/.next",
  "installCommand": "npm install",
  "functions": {
    "app/campaign/[address]/page.tsx": {
      "maxDuration": 30
    },
    "app/campaign/[address]/submit-proof/page.tsx": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/campaign/:address*",
      "destination": "/campaign/[address]"
    }
  ],
  "redirects": [
    {
      "source": "/.well-known/farcaster.json",
      "destination": "https://api.farcaster.xyz/miniapps/hosted-manifest/0198ffe3-6665-f7c5-6552-b056563a2a66",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/campaign/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}
