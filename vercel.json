{
  "buildCommand": "cd front && npm run build",
  "outputDirectory": "front/.next",
  "installCommand": "npm install",
  "functions": {
    "app/campaign/[address]/page.tsx": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/campaign/:address*",
      "destination": "/campaign/[address]"
    }
  ]
}
